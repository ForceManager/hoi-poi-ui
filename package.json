{
    "name": "hoi-poi-ui",
    "version": "0.0.1-alpha.0",
    "main": "dist/index.js",
    "scripts": {
        "lint": "eslint --config .eslintrc --ext js,jsx src/**",
        "precommit": "cross-env NODE_ENV=production lint-staged",
        "start": "start-storybook -p 9001 -c config/storybook",
        "build": "cross-env NODE_ENV=production webpack --mode production --config config/webpack.config.prod.js",
        "test": "jest",
        "test:snapshots": "jest --updateSnapshot",
        "postinstall": "node bin/postinstall",
        "deploy-storybook": "storybook-to-ghpages",
        "build-storybook": "build-storybook -c config/storybook"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn precommit",
            "pre-push": "cross-env yarn test"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "pretty-quick --staged",
            "eslint --fix --config .eslintrc --ext js,jsx",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ForceManager/hoi-poi-ui"
    },
    "author": "ForceManager (https://github.com/ForceManager)",
    "bugs": {
        "url": "https://github.com/ForceManager/hoi-poi-ui/issues"
    },
    "homepage": "https://github.com/ForceManager/hoi-poi-ui",
    "keywords": [
        "library",
        "react",
        "javascript",
        "components",
        "ui"
    ],
    "license": "MIT",
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "devDependencies": {
        "@babel/core": "7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^4.1.9",
        "@storybook/addon-info": "^4.1.7",
        "@storybook/addon-knobs": "^4.1.9",
        "@storybook/addon-links": "^4.1.9",
        "@storybook/addon-options": "^4.1.9",
        "@storybook/addon-viewport": "^4.1.9",
        "@storybook/react": "^4.1.9",
        "@storybook/storybook-deployer": "^2.8.1",
        "@svgr/webpack": "4.1.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.1",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.5",
        "babel-plugin-named-asset-import": "^0.3.0",
        "babel-preset-react-app": "^7.0.0",
        "bfj": "6.1.1",
        "case-sensitive-paths-webpack-plugin": "2.2.0",
        "chalk": "2.4.2",
        "cross-env": "^5.2.0",
        "css-loader": "2.1.0",
        "dotenv": "6.2.0",
        "dotenv-expand": "4.2.0",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.7.1",
        "enzyme-to-json": "^3.3.5",
        "eslint": "5.12.1",
        "eslint-config-prettier": "^3.6.0",
        "eslint-config-react-app": "^3.0.6",
        "eslint-loader": "2.1.1",
        "eslint-plugin-flowtype": "3.2.1",
        "eslint-plugin-import": "2.15.0",
        "eslint-plugin-jest": "^22.1.3",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "7.12.4",
        "file-loader": "3.0.1",
        "fs-extra": "7.0.1",
        "html-webpack-plugin": "4.0.0-beta.5",
        "husky": "^1.3.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "23.6.0",
        "jest-enzyme": "^7.0.1",
        "jest-resolve": "23.6.0",
        "jss": "^9.8.7",
        "jss-isolate": "^5.1.0",
        "lint-staged": "^8.1.0",
        "postcss-flexbugs-fixes": "4.1.0",
        "prettier": "^1.16.1",
        "pretty-quick": "^1.10.0",
        "react": "^16.7.0",
        "react-app-polyfill": "^0.2.0",
        "react-dev-utils": "^7.0.1",
        "react-dom": "^16.7.0",
        "react-jss": "^8.6.1",
        "react-test-renderer": "^16.7.0",
        "resolve": "1.10.0",
        "storybook-addon-jsx": "^6.0.0",
        "terser-webpack-plugin": "1.2.1",
        "url-loader": "1.1.2",
        "webpack": "4.29.0",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "3.1.14",
        "webpack-manifest-plugin": "2.0.4"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "jest": {
        "collectCoverageFrom": [
            "<rootDir>/src/components/**/*.{js,jsx}"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTests.js",
        "testMatch": [
            "<rootDir>/src/components/**/?(*.)+(spec|test).js?(x)"
        ],
        "testEnvironment": "jsdom",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "jsx",
            "node"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    }
}
