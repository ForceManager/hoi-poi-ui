{
    "name": "hoi-poi-ui",
    "version": "0.0.1-alpha.1",
    "main": "dist/index.js",
    "scripts": {
        "lint": "eslint --config .eslintrc --ext js,jsx src/**",
        "precommit": "cross-env NODE_ENV=production lint-staged",
        "start": "styleguidist server",
        "build": "cross-env NODE_ENV=production webpack --mode production --config config/webpack.config.prod.js",
        "test": "jest",
        "test:snapshots": "jest --updateSnapshot",
        "postinstall": "node bin/postinstall",
        "deploy": "cross-env NODE_ENV=production styleguidist build && gh-pages -d styleguide --repo https://$GH_TOKEN@github.com/ForceManager/hoi-poi-ui.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn precommit",
            "pre-push": "yarn test"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "pretty-quick --staged",
            "eslint --fix --config .eslintrc --ext js,jsx",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ForceManager/hoi-poi-ui"
    },
    "author": "ForceManager (https://github.com/ForceManager)",
    "bugs": {
        "url": "https://github.com/ForceManager/hoi-poi-ui/issues"
    },
    "homepage": "https://github.com/ForceManager/hoi-poi-ui",
    "keywords": [
        "library",
        "react",
        "javascript",
        "components",
        "ui"
    ],
    "license": "MIT",
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "devDependencies": {
        "@babel/core": "7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@svgr/webpack": "4.1.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.1",
        "babel-jest": "24.0.0",
        "babel-loader": "8.0.5",
        "babel-plugin-named-asset-import": "^0.3.0",
        "babel-preset-react-app": "^7.0.0",
        "bfj": "6.1.1",
        "case-sensitive-paths-webpack-plugin": "2.2.0",
        "chalk": "2.4.2",
        "cross-env": "^5.2.0",
        "dotenv": "6.2.0",
        "dotenv-expand": "4.2.0",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.9.1",
        "enzyme-to-json": "^3.3.5",
        "eslint": "5.13.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-config-react-app": "^3.0.6",
        "eslint-loader": "2.1.2",
        "eslint-plugin-flowtype": "3.2.1",
        "eslint-plugin-import": "2.16.0",
        "eslint-plugin-jest": "^22.3.0",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "7.12.4",
        "file-loader": "3.0.1",
        "fs-extra": "7.0.1",
        "gh-pages": "^2.0.1",
        "html-webpack-plugin": "4.0.0-beta.5",
        "husky": "^1.3.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "24.0.0",
        "jest-enzyme": "^7.0.1",
        "jest-resolve": "24.0.0",
        "lint-staged": "^8.1.4",
        "postcss-flexbugs-fixes": "4.1.0",
        "prettier": "^1.16.4",
        "pretty-quick": "^1.10.0",
        "react-app-polyfill": "^0.2.0",
        "react-dev-utils": "^7.0.1",
        "react-styleguidist": "^8.0.6",
        "react-test-renderer": "^16.8.4",
        "resolve": "1.10.0",
        "terser-webpack-plugin": "1.2.2",
        "url-loader": "1.1.2",
        "webpack": "4.29.3",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "3.1.14",
        "webpack-manifest-plugin": "2.0.4"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "classnames": "^2.2.6",
        "jss": "^10.0.0-alpha.9",
        "jss-isolate": "^5.1.0",
        "jss-keyframes": "^0.1.0",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-jss": "^10.0.0-alpha.9"
    }
}
