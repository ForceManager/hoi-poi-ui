        "rc-menu": "^8.3.1",
        "rc-progress": "^3.0.0",
        "rc-slider": "^9.3.1",
        "rc-steps": "^4.0.0",
        "rc-tabs": "^10.1.1",
        "rc-tooltip": "^3.7.3",
        "react-animate-height": "^2.0.21",
        "react-circular-progressbar": "^2.0.3",
        "react-collapse": "^5.1.0",
        "react-custom-scrollbars": "^4.2.1",
        "react-dropzone": "^11.0.3",
        "react-flatpickr": "^3.10.4",
        "react-jss": "^10.3.0",
