{
    "name": "hoi-poi-ui",
    "version": "1.2.9",
    "main": "dist/index.js",
    "module": "dist/esm/index.js",
    "scripts": {
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable",
        "prebuild": "rm -rf dist",
        "precommit": "cross-env NODE_ENV=production lint-staged",
        "lint": "eslint --config .eslintrc --ext .js,.jsx src/**",
        "start": "styleguidist server",
        "build": "yarn build:cjs && yarn build:esm && yarn build:es",
        "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./dist --ignore \"**/*.spec.jsx\"",
        "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./dist/esm --ignore \"**/*.spec.jsx\"",
        "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./dist/es --ignore \"**/*.spec.jsx\"",
        "build:watch": "yarn build:esm --watch",
        "release": "yarn build && cp package.json dist/ && cp README.md dist/ && yarn npm login && yarn npm publish && yarn npm logout",
        "release-next": "yarn build && cp package.json dist/ && cp README.md dist/ && yarn npm login && yarn npm publish --tag next && yarn npm logout",
        "test": " cross-env NODE_ENV=test jest",
        "test:snapshots": "cross-env NODE_ENV=test jest --updateSnapshot",
        "build:styleguidist": "cross-env NODE_ENV=production styleguidist build",
        "eslint": "eslint"
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "pretty-quick --staged",
            "eslint --fix --config .eslintrc --ext js,jsx",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ForceManager/hoi-poi-ui"
    },
    "author": "ForceManager (https://github.com/ForceManager)",
    "bugs": {
        "url": "https://github.com/ForceManager/hoi-poi-ui/issues"
    },
    "homepage": "https://github.com/ForceManager/hoi-poi-ui",
    "keywords": [
        "library",
        "react",
        "javascript",
        "components",
        "ui"
    ],
    "license": "MIT",
    "sideEffects": false,
    "peerDependencies": {
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
